{
  "name": "monorepo",
  "version": "1.0.0",
  "description": "study monorepo",
  "main": "index.js",
  "author": "song",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start:first": "cd packages/first && vite",
    "start:twice": "cd packages/twice && npm run start",
    "install:all": "npm install && cd packages/first && npm install && cd ../twice && npm install",
    "eslint": "eslint --ext .ts,.tsx,.js,.jsx --fix --ignore-path .gitignore node_modules/",
    "prettier": "prettier --write /packages/**/*.{ts,tsx,js,jsx}",
    "prepare": "husky"
  },
  "lint-staged": {
    "packages/twice/src/**/*.{js,ts,jsx,tsx}": [
      "npx eslint --fix --max-warnings=5"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.0",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@types/react": "^18.2.52",
    "@types/react-dom": "^18.2.18",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "antd": "^5.14.0",
    "css-loader": "^6.10.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^9.0.11",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "style-loader": "^3.3.4",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  },
  "volta": {
    "node": "18.12.0"
  }
}
